{
  "name": "file-download-network",
  "main": "build/server",
  "license": "MIT",
  "scripts": {
    "build": "npm run build:remix && npm run build:server",
    "build:remix": "remix build && cp ./src/public/* ./build/public",
    "build:server": "tsc -p ./src/server/tsconfig.json",
    "dev": "remix dev -c \"npm start\"",
    "lint": "eslint . --max-warnings=0",
    "lint:fix": "eslint . --fix",
    "start": "node ."
  },
  "dependencies": {
    "@remix-run/css-bundle": "2.9.2",
    "@remix-run/express": "2.9.2",
    "@remix-run/node": "2.9.2",
    "@remix-run/react": "2.9.2",
    "compression": "1.7.4",
    "dotenv": "16.4.5",
    "express": "4.19.2",
    "express-rate-limit": "7.2.0",
    "helmet": "7.1.0",
    "isbot": "5.1.13",
    "morgan": "1.10.0",
    "react": "18.3.1",
    "react-dom": "18.3.1"
  },
  "devDependencies": {
    "@remix-run/dev": "2.9.2",
    "@total-typescript/ts-reset": "0.5.1",
    "@tsconfig/node18": "18.2.4",
    "@tsconfig/remix": "2.0.5",
    "@tsconfig/strictest": "2.0.5",
    "@types/express": "4.17.21",
    "@types/morgan": "1.9.9",
    "@types/node": "18.19.43",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "eslint": "8.57.0",
    "eslint-config-promise": "github:promise/eslint-config",
    "tailwindcss": "3.4.7",
    "typescript": "5.5.4"
  },
  "eslintConfig": {
    "extends": "promise",
    "rules": {
      "no-console": "off",
      "react/react-in-jsx-scope": "off"
    }
  }
}
